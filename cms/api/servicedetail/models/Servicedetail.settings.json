{
  "connection": "default",
  "collectionName": "servicedetails",
  "info": {
    "name": "servicedetail",
    "description": "Service Details"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "unique": true,
      "required": true,
      "type": "string"
    },
    "slug": {
      "unique": true,
      "required": true,
      "type": "string"
    },
    "introductionText": {
      "type": "text"
    },
    "toolListHeading": {
      "type": "string"
    },
    "toolListText": {
      "type": "text"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "serviceoverview": {
      "model": "serviceoverview",
      "via": "servicedetails"
    },
    "accountmanagers": {
      "collection": "accountmanager",
      "via": "servicedetails"
    }
  }
}