{
  "connection": "default",
  "collectionName": "servicelists",
  "info": {
    "name": "servicelist",
    "description": "Service List"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "Name": {
      "unique": true,
      "required": true,
      "type": "string"
    },
    "Introduction-Text": {
      "type": "text"
    },
    "Tool-List-Heading": {
      "type": "string"
    },
    "Tool-List-Text": {
      "type": "text"
    },
    "serviceoverview": {
      "model": "serviceoverview",
      "via": "servicelists"
    },
    "Image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "Slug": {
      "unique": true,
      "required": true,
      "type": "string"
    }
  }
}